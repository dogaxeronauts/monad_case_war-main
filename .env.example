# Environment Variables for Auto Auction System

# Game Wallet Configuration
GAME_WALLET_PRIVATE_KEY=your_game_wallet_private_key_here

# Contract Addresses
AUTO_AUCTION_CONTRACT_ADDRESS=your_deployed_auction_contract_address_here
TREASURY_CONTRACT_ADDRESS=your_treasury_contract_address_here

# Monad Testnet Configuration
VITE_MONAD_RPC_URL=https://rpc.testnet.monad.xyz
VITE_MONAD_CHAIN_ID=84532

# Player Configuration
VITE_DEFAULT_PLAYER_ADDRESS=your_default_player_address_here

# Auction Settings
VITE_DEFAULT_BID_INCREMENT_PERCENTAGE=10
VITE_AUTO_BID_DELAY_MS=1000
VITE_MAX_GAS_PRICE_GWEI=50

# Debug Settings
VITE_DEBUG_MODE=false
VITE_ENABLE_AUCTION_LOGS=true